{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "es2015",
    "types": [],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@common/*": [
        "../src/app/modules/common/*"
      ],
      "@weather/*": [
        "../src/app/modules/weather/*"
      ],
      "@shared/*": [
        "../src/app/shared/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
